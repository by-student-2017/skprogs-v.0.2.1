Geometry = VaspFormat {
  <<< "POSCAR"
}

Hamiltonian = DFTB {
  Scc = Yes
  SccTolerance = 1e-5
  SlaterKosterFiles = Type2FileNames {
    Prefix = "./"
    Separator = "-"
    Suffix = ".skf"
  }
  MaxAngularMomentum {
    Hf = "d"
  }
  KPointsAndWeights = SupercellFolding {
    8 0 0
    0 8 0
    0 0 4
    0.0 0.0 0.5
  }
}

ParserOptions {
  ParserVersion = 12
}
